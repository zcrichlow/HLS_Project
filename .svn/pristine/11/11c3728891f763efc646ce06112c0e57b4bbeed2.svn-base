package edu.psgv.sweng861;

import org.apache.logging.log4j.LogManager;
import org.apache.logging.log4j.Logger;

/*
 * SimpleP implements Playlist
 */
public class SimpleP extends Playlist {
	
	private static final Logger logger = LogManager.getLogger();		
			
	@Override
	public String toString() {
		String str = "\n\n\n\t\tSimple Playlist\n";
		str = "Playlist URL: " + this.url + "\nPlaylist Type: Simple" 
		+ "\nPlaylist Content: " + this.urlContent + "\n";			
		return str;
	}
}	